#include <stdio.h>
int main()
{
	int n,i,a[20];
	printf("Enter the number of elements: ");
	scanf("%d",&n);
	for(i=0;i<=n;i++)
	{
		scanf("%d",&a[i]);
	}
	printf("Even numbers in the list\n");
	for(i=0;i<=n;i++)
	{
		if(a[i]%2==0)
	printf("%d",a[i]);
}
	printf("\nOdd numbers in the list\n");
	for(i=0;i<=n;i++)
	{
		if(a[i]%2==1)
	printf("%d",a[i]);
}
	return 0;
}
